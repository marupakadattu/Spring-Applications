<?xml version="1.0" encoding="UTF-8"?>
<beans
    xmlns="http://www.springframework.org/schema/beans" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xmlns:p="http://www.springframework.org/schema/p" 
    xsi:schemaLocation="http://www.springframework.org/schema/beans 
               http://www.springframework.org/schema/beans/spring-beans-3.0.xsd"> 
 
<bean id="employeebean" class="models.Employee">
<property name="empNo" value="1"></property>
<property name="eName" value="Hema"></property>
<property name="salary" value="25000"></property>
<property name="department" ref ="depat1"></property>
</bean>
<bean id="depat1" class="models.Department">
<property name="deptNo" value="100"></property>
<property name="deptName" value="HR"></property>
</bean>
</beans>